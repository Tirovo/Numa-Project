{
  "model": "vits",
  "run_name": "vits_numa_expressive_v2",
  "output_path": "runs/vits_numa_v2",

  "sample_rate": 22050,
  "fft_size": 1024,
  "win_length": 1024,
  "hop_length": 256,
  "num_mels": 80,
  "mel_fmin": 0,
  "mel_fmax": [8000.0],

  "batch_group_size": 512,
  "batch_size": 16,
  "eval_batch_size": 16,
  "epochs": 200,
  "lr": 0.0001,
  "grad_clip": [1.0, 1.0, 1.0],
  "run_eval": true,
  "eval_interval": 5,
  "print_step": 25,
  "save_step": 100,
  "save_n_checkpoints": 3,
  "keep_last_epochs": 2,
  "num_loader_workers": 12,
  "ignore_errors": true,

  "characters": {
    "pad": "_",
    "eos": "~",
    "bos": "^",
    "blank": "<BLANK>",
    "characters": " !\"',-.:?ABCDEFGHIJKLMNOPQRSTUVWXYZabcdefghijklmnopqrstuvwxyz",
    "punctuations": "",
    "phonemes": "",
    "is_unique": true,
    "is_sorted": true
  },

  "text_cleaner": "english_cleaners",
  "use_phonemes": false,
  "use_audio_input": false,

  "datasets": [
    {
      "name": "numa_voice_dataset",
      "path": "../numa-voice-dataset-highpitch-sox",
      "meta_file_train": "metadata.csv",
      "meta_file_val": "metadata.csv",
      "formatter": "ljspeech",
      "ignored_speakers": []
    }
  ],

  "vocoder": "hifigan",
  "vocoder_config": null,
  "vocoder_checkpoint": "vocoder_models/hifigan/vctk/checkpoint_500000.pth.tar",
  "vocoder_speaker_id": null,
  "vocoder_type": "hifigan",
}
